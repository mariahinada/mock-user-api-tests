{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["ES2020"],
    "esModuleInterop": true,
    "strict": true,
    "types": ["cypress", "node"],
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": ["cypress/**/*.ts", "cypress.config.ts"]
}
